{
  "db_name": "PostgreSQL",
  "query": "SELECT DISTINCT p.id, p.name, p.description, p.category, p.created_at, p.updated_at\n        FROM permissions p\n        INNER JOIN role_permissions rp ON p.id = rp.permission_id\n        INNER JOIN user_roles ur ON rp.role_id = ur.role_id\n        WHERE ur.user_id = $1\n        ORDER BY p.category, p.name",
