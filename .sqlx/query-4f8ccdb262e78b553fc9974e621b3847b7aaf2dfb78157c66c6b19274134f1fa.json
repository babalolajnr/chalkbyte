{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                b.id,\n                b.name,\n                b.description,\n                b.level_id,\n                b.created_at,\n                b.updated_at,\n                COUNT(u.id) as \"student_count!\"\n            FROM branches b\n            INNER JOIN levels l ON l.id = b.level_id\n            LEFT JOIN users u ON u.branch_id = b.id AND u.role = 'student'\n            WHERE b.id = $1 AND l.school_id = $2\n            GROUP BY b.id\n            ",
